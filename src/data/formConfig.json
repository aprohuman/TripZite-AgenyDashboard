{
  "sections": [
    {
      "id": "trip-details",
      "title": "Trip Details",
      "fields": [
        {
          "id": "start-city",
          "label": "1st Country / State / City",
          "type": "text",
          "required": true,
          "validation": { "minLength": 3, "maxLength": 50 }
        },
        {
          "id": "end-trace",
          "label": "+ Add (End)Trace",
          "type": "repeatable",
          "fields": [
            {
              "id": "city",
              "type": "text",
              "placeholder": "Enter city"
            }
          ]
        }
      ]
    },
    {
      "id": "trip-breakdown",
      "title": "Trip Breakdown",
      "fields": [
        {
          "id": "length",
          "label": "Length",
          "type": "number",
          "validation": { "min": 1, "max": 100 }
        },
        {
          "id": "accessories",
          "type": "nested",
          "fields": [
            {
              "id": "sorted-accessories",
              "label": "Accessories sorted",
              "type": "checkbox-group",
              "options": ["Toward", "Not", "Binary"]
            }
          ]
        }
      ]
    }
  ]
}
